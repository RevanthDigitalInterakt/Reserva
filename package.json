    "@react-native-firebase/firestore": "^18.1.0",
    "@react-native-firebase/in-app-messaging": "^18.1.0",
    "@react-native-firebase/installations": "^18.1.0",
    "@react-native-firebase/messaging": "^18.1.0",
    "@react-native-firebase/perf": "^18.1.0",
    "@react-native-firebase/remote-config": "^18.1.0",
    "@react-native-firebase/storage": "^18.1.0",
