    "yup": "^1.2.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/config-conventional": "^17.1.0",
    "@evilmartians/lefthook": "^1.1.3",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@rnx-kit/align-deps": "^2.2.4",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^3.0.2",
    "@types/gl-react": "^3.15.4",
    "@types/jest": "29.5.3",
    "@types/node": "^20.4.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.14",
    "@types/react-native": "^0.72.2",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-countdown-component": "^2.7.0",
    "@types/react-native-fetch-blob": "^0.10.7",
    "@types/react-test-renderer": "18.0.0",
    "@types/sha1": "^1.1.3",
    "@types/styled-components": "5.1.26",
    "@types/styled-components-react-native": "^5.1.1",
    "@types/styled-system": "5.1.16",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "babel-jest": "^29.6.1",
    "commitlint": "^17.1.2",
    "eslint": "8.44.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest": "^29.6.1",
    "jest-mock-axios": "^4.7.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "patch-package": "^7.0.2",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.2.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6",
    "visualize-bundle": "^1.4.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/src/**/*.(test).{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./__mocks__/extend.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!((jest-)?@react-native|react-native|@usereservaapp/reserva-ui|react-native-modal|react-native-progress|react-native-animatable|lottie-react-native)"
    ],
    "automock": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.(ts|tsx)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/*/*/navigation",
      "<rootDir>/src/routes"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/base/styles/icons.ts"
    },
    "moduleDirectories": [
      "<rootDir>/../",
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 16,
        "functions": 17,
        "lines": 22,
        "statements": 22
      }
    }
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.64"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "clipboard",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "masked-view",
        "modal",
        "navigation/native",
        "navigation/stack",
        "netinfo",
        "react",
        "react-dom",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg",
        "webview"
      ]
    }
  },
  "engines": {
    "node": ">=16"
  }
}