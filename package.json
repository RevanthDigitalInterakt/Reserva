{
  "name": "reserva",
  "version": "2.27.0",
  "private": true,
  "scripts": {
    "setup-ios-permissions": "react-native setup-ios-permissions && pod-install",
    "android": "react-native run-android",
    "android-release": "npx react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --passWithNoTests --detectOpenHandles --forceExit --silent",
    "test:watch": "jest --passWithNoTests --detectOpenHandles --forceExit --watch",
    "lint": "eslint . --ext .ts --ext .tsx --cache --fix",
    "clean": "cd android && ./gradlew clean && cd .. && cd ios && pod deintegrate && pod cache clean --all && pod install && cd ..",
    "android-build-release": "cd android && ./gradlew assembleRelease",
    "android-build-aab": "cd android && ./gradlew bundleRelease",
    "codepush-android:dev": "appcenter codepush release-react -a Reserva-App/reserva-app-1 -d Staging",
    "codepush-android:prod": "appcenter codepush release-react -a Reserva-App/reserva-app-1 -d Production",
    "codepush-ios:dev": "appcenter codepush release-react -a Reserva-App/reserva-app -d Staging",
    "codepush-ios:prod": "appcenter codepush release-react -a Reserva-App/reserva-app -d Production",
    "postinstall": "patch-package && yarn setup-ios-permissions",
    "bundle:view": "visualize-bundle",
    "codegen": "graphql-codegen --config codegen.ts",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write",
    "pipeline:semantic-release-pre:prod": "semantic-release --extends ./semantic-release-pre.js --no-ci --debug",
    "pipeline:semantic-release-post:prod": "semantic-release --extends ./semantic-release-post.js  --no-ci --debug"
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@datadog/mobile-react-native": "2.4.3",
    "@gorhom/bottom-sheet": "^4",
    "@notifee/react-native": "^7.8.2",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-native-firebase/analytics": "18.9.0",
    "@react-native-firebase/app": "18.9.0",
    "@react-native-firebase/app-check": "18.9.0",
    "@react-native-firebase/crashlytics": "18.9.0",
    "@react-native-firebase/firestore": "18.9.0",
    "@react-native-firebase/installations": "18.9.0",
    "@react-native-firebase/messaging": "18.9.0",
    "@react-native-firebase/perf": "18.9.0",
    "@react-native-firebase/remote-config": "18.9.0",
    "@react-native-firebase/storage": "18.9.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "axios": "1.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "formik": "^2.4.2",
    "graphql": "^16.7.1",
    "jail-monkey": "^2.8.0",
    "jest-fetch-mock": "3.0.3",
    "lottie-react-native": "^6.7.0",
    "obfuscator-io-metro-plugin": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.12",
    "react-native-appsflyer": "6.12.2",
    "react-native-auto-height-image": "^3.2.4",
    "react-native-background-timer": "^2.4.1",
    "react-native-bootsplash": "^4.7.5",
    "react-native-clarity": "0.0.5",
    "react-native-code-push": "8.0.2",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.8.0",
    "react-native-drop-shadow": "^0.0.8",
    "react-native-exit-app": "^2.0.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-get-random-values": "1.9.0",
    "react-native-image-picker": "^5.6.1",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal": "^13.0.1",
    "react-native-onesignal": "4.5.2",
    "react-native-permissions": "^3.8.4",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "3.3.0",
    "react-native-reanimated-carousel": "3.5.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.23.0",
    "react-native-sha256": "^1.4.10",
    "react-native-share": "9.2.3",
    "react-native-store-version": "^1.4.1",
    "react-native-svg": "13.14.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-tracking-transparency": "^0.1.2",
    "react-native-url-polyfill": "2.0.0",
    "react-native-uuid": "^2.0.1",
    "react-native-ux-cam": "^6.0.1",
    "react-native-video": "6.0.0-beta.6",
    "react-native-webview": "^13.3.1",
    "sha1": "^1.1.1",
    "styled-components": "^6.0.5",
    "styled-system": "^5.1.5",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "yup": "^1.2.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/runtime": "^7.22.6",
    "@commitlint/config-conventional": "^17.6.7",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@react-native/metro-config": "0.72.12",
    "@rnx-kit/align-deps": "^2.2.4",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.2",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.1.3",
    "@tsconfig/react-native": "^3.0.2",
    "@types/gl-react": "^3.15.4",
    "@types/jest": "29.5.3",
    "@types/node": "^20.4.5",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.17",
    "@types/react-native": "^0.72.2",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-countdown-component": "^2.7.0",
    "@types/react-native-fetch-blob": "^0.10.7",
    "@types/react-native-video": "^5.0.15",
    "@types/react-test-renderer": "18.0.0",
    "@types/sha1": "^1.1.3",
    "@types/styled-components": "5.1.26",
    "@types/styled-components-react-native": "^5.2.1",
    "@types/styled-system": "5.1.16",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "babel-jest": "^29.6.2",
    "commitlint": "^17.6.7",
    "eslint": "8.46.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "jest": "^29.6.2",
    "jest-mock-axios": "^4.7.2",
    "metro-react-native-babel-preset": "0.76.9",
    "patch-package": "^8.0.0",
    "pod-install": "^0.1.39",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.2.0",
    "semantic-release": "^23.0.0",
    "semantic-release-react-native": "^1.8.2",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6",
    "visualize-bundle": "^1.4.0"
  },
  "reactNativePermissionsIOS": [
    "PhotoLibrary",
    "PhotoLibraryAddOnly"
  ],
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "<rootDir>/src/**/*.(test).{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./__mocks__/extend.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!((jest-)?@react-native|react-native|react-native-modal|react-native-progress|lottie-react-native|react-native-config)",
      "jest-runner"
    ],
    "automock": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.(ts|tsx)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/*/*/navigation",
      "<rootDir>/src/routes"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/base/styles/icons.ts"
    },
    "moduleDirectories": [
      "<rootDir>/../",
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 15,
        "functions": 16,
        "lines": 21,
        "statements": 21
      }
    }
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.64"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "clipboard",
        "core",
        "core-android",
        "core-ios",
        "gestures",
        "jest",
        "masked-view",
        "modal",
        "navigation/native",
        "navigation/stack",
        "netinfo",
        "react",
        "react-dom",
        "react-test-renderer",
        "safe-area",
        "screens",
        "storage",
        "svg",
        "webview"
      ]
    }
  },
  "engines": {
    "node": "^20.x.x"
  }
}
